# Server Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# LLM Configuration
GOOGLE_GENAI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here_for_fallback

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=3600

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=langgraph_vectors

# Auth0 Configuration
AUTH0_DOMAIN=your-auth0-domain.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret
AUTH0_AUDIENCE=https://your-api-audience

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# MCP Tool Servers
MCP_BROWSER_SEARCH_URL=http://localhost:8080
MCP_SQL_QUERY_URL=http://localhost:8081
MCP_TIMEOUT=30000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=app.log

# Security
CORS_ORIGIN=http://localhost:3001
HELMET_CSP_DIRECTIVES=default-src 'self'

# Telemetry
ENABLE_TELEMETRY=true
TELEMETRY_ENDPOINT=http://localhost:4318 